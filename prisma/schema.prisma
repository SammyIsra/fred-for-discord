generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-arm-openssl-1.1.x"]
}

datasource db {
  provider = "postgresql"
  url      = "postgres://fred-bot:secret@192.168.86.32:5432/fred-bot"
}

model Keys {
  name    String  @unique
  dungeon String
  level   Int
}

model Users {
  name    String  @unique
  email   String
}
